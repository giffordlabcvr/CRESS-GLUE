delete project circoviridae

create project circoviridae "A GLUE project for the Circoviridae" --minVersion 0.1.66

schema-project circoviridae

  run file glue/core/circoviridaeSchemaExtensions.glue
  exit

# Build core project
run file glue/build/buildCoreProject.glue
  
# Build endogenous circovirus component
run file glue/build/buildEveExtension.glue

# Inherit features from HIV-1 master reference to other reference sequences
run script glue/core/circoviridaeInheritFeatureLocations.js

