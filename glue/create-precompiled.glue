module fastaProteinExporter export member AL_UNCONSTRAINED_Circovirus_genome -r REF_PCV-3 -f Rep -a -o precompiled/circovirus-rev.faa
module fastaProteinExporter export member AL_UNCONSTRAINED_Circovirus_genome -r REF_PCV-3 -f Cap-rev -a -o precompiled/circovirus-cap.faa
