delete project cress

create project cress "A GLUE project for the circular Rep-encoding single-stranded DNA viruses" --minVersion 0.1.66

schema-project cress

  run file glue/core/cressSchemaExtensions.glue
  exit

# Build core project
run file glue/build/buildCoreProject.glue
  
# Build endogenous circovirus component
run file glue/build/buildEveExtension.glue

# Inherit features from HIV-1 master reference to other reference sequences
run script glue/core/cressInheritFeatureLocations.js

